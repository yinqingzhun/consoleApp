<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="enyim.com">
      <section name="memcached" type="Enyim.Caching.Configuration.MemcachedClientSection, Enyim.Caching" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="MyUrls" type="Samples.AspNet.UrlsSection, ConfigurationElement, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" allowDefinition="Everywhere" allowExeDefinition="MachineToApplication" restartOnExternalChanges="true" />
  </configSections>
  <MyUrls name="MyFavorites">
    <simple name="Microsoft1" url="http://www.microsoft1.com" port="1" />
    <urls>
      <clear />
      <add name="Microsoft2" url="http://www.microsoft2.com" port="2" />
      <add name="Contoso" url="http://www.contoso.com" port="8080" />
    </urls>
  </MyUrls>
  <connectionStrings>
    <add name="aohEntities" connectionString="metadata=res://*/AohModel.csdl|res://*/AohModel.ssdl|res://*/AohModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=192.168.200.29;initial catalog=aoh;persist security info=True;user id=sa;password=1qaz@WSX;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  <system.serviceModel>
    <bindings />
    <client />
  </system.serviceModel>
  <enyim.com protocol="Binary">
    <memcached>
      <servers>
        <add address="127.0.0.1" port="11211" />
        <add address="127.0.0.1" port="11212" />
      </servers>
      <socketPool minPoolSize="1" maxPoolSize="10" connectionTimeout="00:00:10" deadTimeout="00:02:00" />
    </memcached>
  </enyim.com>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}]- %message%newline" />
      </layout>
    </appender>
    <root>
      <priority value="WARN" />
      <appender-ref ref="ConsoleAppender">
        <filter type="log4net.Filter.LevelRangeFilter">
          <levelMin value="WARN" />
          <levelMax value="FATAL" />
        </filter>
      </appender-ref>
    </root>
  </log4net>
  <appSettings>
    <add key="MqConcurrentSizeInSharingMode" value="10" />
    <add key="MqConcurrentSizeInExclusiveMode" value="3" />
    <add key="Service.RabitMq.Host" value="yinqz-7,chenshunliang-3" />
    <add key="Service.RabitMq.User" value="mq" />
    <add key="Service.RabitMq.Password" value="mq" />
    <!--发送邮件-->
    <add key="ExceptionStatLog_SendMailInterval" value="86400000" />
    <add key="ExceptionStatLog_SenderEmailAddress" value="aoh_notice@dm.bitauto.com" />
    <add key="ExceptionStatLog_ReceiverEmailAddress" value="yinqz@yiche.com" />
    <add key="ExceptionStatLog_SmtpHost" value="59.151.102.80" />
    <add key="ExceptionStatLog_SmtpPort" value="25" />
    <add key="ExceptionStatLog_UserName" value="aoh_notice@dm.bitauto.com" />
    <add key="ExceptionStatLog_Password" value="aoh@2015" />
  </appSettings>
